    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_58872_needit.GetNewEmail</api_name>
        <client_callable>true</client_callable>
        <description>Client Callable AJAX based Script to retrieve Email IDs from sys_user table</description>
        <name>GetNewEmail</name>
        <script><![CDATA[var GetNewEmail = Class.create();
GetNewEmail.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	getEmail: function(){
		var userRecord=new GlideRecord('sys_user');
		userRecord.get(this.getParameter('sysparm_userID'));
		return userRecord.email + '';
	},
    type: 'GetNewEmail'
});]]></script>
